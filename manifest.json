{
  "manifest_version": 3,
  "name": "YouTube Old Contents",
  "description": "For getting YouTube contents URLs ordered by upload datetime",
  "version": "1.0",
  "icons": {
    "16": "images/sort-by-numeric-order_icon-icons.com_32.png",
    "32": "images/sort-by-numeric-order_icon-icons.com_32.png",
    "48": "images/sort-by-numeric-order_icon-icons.com_48.png",
    "128": "images/sort-by-numeric-order_icon-icons.com_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://www.youtube.com/*/videos"
  ]
}